<?xml version="1.0" encoding="utf-8"?>
<com.google.android.apps.messaging.ui.search.resultsview.StarredMessagesResultItemView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/swipeableContainer"
    android:clipChildren="false"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:foreground="@drawable/conversation_list_item_view_fg"
    android:minHeight="@dimen/conversation_list_item_view_min_height_v2"
    android:importantForAccessibility="yes"
    style="@style/ConversationListItemViewPaddingStyleV2">
    <FrameLayout
        android:id="@+id/contact_avatar_view"
        android:background="@android:color/transparent"
        android:clipChildren="false"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/conversation_text_content">
        <ImageView
            android:id="@+id/conversation_icon"
            android:background="@android:color/transparent"
            android:layout_width="@dimen/conversation_list_contact_icon_size"
            android:layout_height="@dimen/conversation_list_contact_icon_size"
            android:contentDescription="@null"
            android:importantForAccessibility="no"/>
        <ViewStub
            android:id="@+id/conversation_checkmark"
            android:inflatedId="@+id/conversation_checkmark"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"/>
    </FrameLayout>
    <RelativeLayout
        android:id="@+id/conversation_text_content"
        android:background="@android:color/transparent"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/attachment_and_timestamp"
        app:layout_constraintStart_toEndOf="@+id/contact_avatar_view"
        app:layout_constraintTop_toTopOf="parent"
        style="@style/ConversationListItemViewTextContentPaddingStyleV3">
        <ImageView
            android:id="@+id/conversation_notification_bell"
            android:background="@android:color/transparent"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_notifications_off_small_light"
            android:tint="?android:attr/textColorPrimary"
            android:contentDescription="@null"
            android:importantForAccessibility="no"
            android:layout_alignParentStart="true"
            style="@style/ConversationListNotificationBellPaddingStyle"/>
        <ViewStub
            android:id="@+id/work_profile_icon"
            android:inflatedId="@+id/work_profile_icon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toEndOf="@+id/conversation_notification_bell"/>
        <android.support.v7.widget.AppCompatTextView
            android:textAppearance="?attr/textAppearanceHeadline6"
            android:textSize="16sp"
            android:textColor="?attr/colorOnSurface"
            android:layout_gravity="center_vertical"
            android:id="@+id/conversation_name"
            android:paddingBottom="2dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:singleLine="true"
            android:layout_alignParentTop="true"
            android:layout_toEndOf="@+id/work_profile_icon"/>
        <ViewStub
            android:id="@+id/conversation_subject"
            android:inflatedId="@+id/conversation_subject"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/conversation_name"/>
        <androidx.emoji.widget.EmojiAppCompatTextView
            android:id="@+id/conversation_snippet"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/conversation_subject"
            android:textAlignment="viewStart"
            style="@style/ConversationListItemViewTextStyleV3"/>
        <ViewStub
            android:id="@+id/additional_info_icon"
            android:inflatedId="@+id/additional_info_icon"
            android:layout_width="@dimen/conversation_list_additional_icon_size"
            android:layout_height="@dimen/conversation_list_additional_icon_size"
            android:layout_below="@+id/conversation_snippet"
            android:layout_marginEnd="@dimen/conversation_list_icon_margin"/>
        <ViewStub
            android:id="@+id/additional_text"
            android:inflatedId="@+id/additional_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/conversation_snippet"
            android:layout_toEndOf="@+id/additional_info_icon"/>
        <ViewStub
            android:id="@+id/conversation_status"
            android:layout="@layout/conversation_status_view"
            android:inflatedId="@+id/conversation_status"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/additional_text"/>
    </RelativeLayout>
    <FrameLayout
        android:layout_gravity="end|top"
        android:id="@+id/attachment_and_timestamp"
        android:background="@android:color/transparent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/conversation_text_content">
        <LinearLayout
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:id="@+id/conversation_timestamp_and_unread_badge"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <android.support.v7.widget.AppCompatTextView
                android:textAppearance="?attr/textAppearanceBody1"
                android:textSize="12sp"
                android:ellipsize="end"
                android:id="@+id/conversation_timestamp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:maxWidth="150dp"
                android:singleLine="true"/>
            <ViewStub
                android:id="@+id/unread_badge_view_with_message_count_stub"
                android:layout="@layout/unread_badge_with_message_count"
                android:inflatedId="@+id/unread_badge"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="12dp"/>
        </LinearLayout>
    </FrameLayout>
    <include
        android:id="@+id/conversation_list_item_star_icon"
        android:layout_width="@dimen/conversation_list_item_view_star_wrapper_size"
        android:layout_height="@dimen/conversation_list_item_view_star_wrapper_size"
        android:layout_marginTop="6dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/attachment_and_timestamp"
        layout="@layout/conversation_list_star_icon"/>
    <ImageView
        android:id="@+id/conversation_image_preview"
        android:background="@android:color/transparent"
        android:visibility="gone"
        android:layout_width="@dimen/conversation_list_image_preview_size"
        android:layout_height="@dimen/conversation_list_image_preview_size"
        android:scaleType="centerCrop"
        android:contentDescription="@string/message_image_content_description"/>
    <ImageView
        android:layout_gravity="center"
        android:id="@+id/conversation_video_overlay_image"
        android:visibility="gone"
        android:layout_width="@dimen/conversation_list_video_overlay_size"
        android:layout_height="@dimen/conversation_list_video_overlay_size"
        android:src="@drawable/quantum_gm_ic_play_circle_filled_white_24"
        android:contentDescription="@null"/>
</com.google.android.apps.messaging.ui.search.resultsview.StarredMessagesResultItemView>
