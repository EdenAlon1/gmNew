<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/zero_state_search_video_result_item_frame_layout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <ImageView
        android:id="@+id/zero_state_search_thumbnail"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:contentDescription="@string/zero_state_search_video_item_description"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <ViewStub
        android:id="@+id/zero_state_search_video_star_icon"
        android:layout="@layout/zero_state_search_result_star_icon"
        android:inflatedId="@+id/zero_state_search_video_star_icon"
        android:layout_width="@dimen/zero_state_search_result_star_icon_size"
        android:layout_height="@dimen/zero_state_search_result_star_icon_size"
        android:layout_marginTop="@dimen/zero_state_search_result_star_icon_top_margin"
        android:layout_marginEnd="@dimen/zero_state_search_result_star_icon_end_margin"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <ViewStub
        android:id="@+id/zero_state_search_video_play_icon"
        android:layout="@layout/zero_state_search_video_play_icon"
        android:inflatedId="@+id/zero_state_search_video_play_icon"
        android:layout_width="@dimen/video_play_icon_size"
        android:layout_height="@dimen/video_play_icon_size"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <com.google.android.apps.messaging.ui.video.VideoOverlayView
        android:id="@+id/search_video_overlay"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/zero_state_search_video_duration_margin_bottom"
        android:importantForAccessibility="no"
        android:layout_marginStart="@dimen/zero_state_search_video_duration_margin_start_end"
        android:layout_marginEnd="@dimen/zero_state_search_video_duration_margin_start_end"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>
