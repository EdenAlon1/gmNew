<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <ImageView
        android:id="@+id/zero_state_search_photo_thumbnail"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:contentDescription="@string/zero_state_search_photo_item_description"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <ViewStub
        android:id="@+id/zero_state_search_photo_star_icon"
        android:layout="@layout/zero_state_search_result_star_icon"
        android:inflatedId="@+id/zero_state_search_photo_star_icon"
        android:layout_width="@dimen/zero_state_search_result_star_icon_size"
        android:layout_height="@dimen/zero_state_search_result_star_icon_size"
        android:layout_marginTop="@dimen/zero_state_search_result_star_icon_top_margin"
        android:layout_marginEnd="@dimen/zero_state_search_result_star_icon_end_margin"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
</androidx.constraintlayout.widget.ConstraintLayout>
