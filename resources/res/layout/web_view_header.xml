<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android">
    <ImageView
        android:layout_gravity="start|center_vertical"
        android:id="@+id/web_view_display_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:contentDescription="@null"
        android:filterTouchesWhenObscured="true"
        android:layout_marginStart="@dimen/web_view_icon_margin_left"/>
    <TextView
        android:textSize="14sp"
        android:textColor="@color/google_grey800"
        android:layout_gravity="center_vertical"
        android:id="@+id/web_view_title"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:filterTouchesWhenObscured="true"
        android:fontFamily="@font/google_sans"
        android:layout_marginStart="@dimen/web_view_title_margin_left"/>
    <ImageView
        android:layout_gravity="end|center_vertical"
        android:id="@+id/web_view_dismiss_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/quantum_gm_ic_clear_black_24"
        android:contentDescription="@string/lighter_close_button_label"
        android:filterTouchesWhenObscured="true"
        android:layout_marginEnd="@dimen/web_view_dismiss_button_margin_right"/>
</merge>
