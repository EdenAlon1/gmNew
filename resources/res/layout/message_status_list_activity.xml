<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/message_status_list_container"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <fragment
        android:id="@+id/message_status_list_fragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        class="com.google.android.apps.messaging.ui.conversation.message.statuschangelist.MessageStatusListFragment"/>
    <com.google.android.apps.messaging.ui.toolbar.BugleToolbar
        android:theme="@style/ThemeOverlay.Bugle.ActionBar"
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:minHeight="?attr/actionBarSize"
        app:theme="@style/ThemeOverlay.Bugle.ActionBar"
        app:titleTextAppearance="@style/BugleToolbarTitleTextStyle"/>
</FrameLayout>
