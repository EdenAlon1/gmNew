<?xml version="1.0" encoding="utf-8"?>
<com.google.android.apps.messaging.ui.mediapicker.c2o.content.ContainerContentView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/c2o_content_category_container"
    style="@style/CategoryStyle">
    <com.google.android.apps.messaging.ui.mediapicker.c2o.content.CategoryToggleView
        android:id="@+id/c2o_category_toggle"
        android:importantForAccessibility="no"
        style="@style/CategoryToggleViewStyle">
        <FrameLayout
            android:layout_gravity="center_vertical"
            android:id="@+id/c2o_category_toggle_frame"
            android:background="@drawable/compose2o_category_toggle_background_m2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:foreground="?attr/selectableItemBackground"
            style="@style/CategoryNotImportantForAccessibilityStyle"/>
        <ImageView
            android:layout_gravity="center"
            android:id="@+id/c2o_category_toggle_icon"
            android:layout_width="@dimen/category_toggle_icon_width"
            android:layout_height="@dimen/category_toggle_icon_height"
            android:tint="?attr/colorPrimaryBrandIcon"
            style="@style/CategoryNotImportantForAccessibilityStyle"/>
    </com.google.android.apps.messaging.ui.mediapicker.c2o.content.CategoryToggleView>
    <FrameLayout
        android:id="@+id/c2o_category_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/category_content_item_margin"
        android:layout_marginBottom="@dimen/category_content_item_margin"
        android:layout_marginStart="@dimen/category_content_item_margin"
        android:layout_marginEnd="@dimen/category_container_margin_end"/>
</com.google.android.apps.messaging.ui.mediapicker.c2o.content.ContainerContentView>
