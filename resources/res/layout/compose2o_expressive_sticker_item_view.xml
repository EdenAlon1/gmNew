<?xml version="1.0" encoding="utf-8"?>
<com.google.android.apps.messaging.ui.mediapicker.c2o.expressivesticker.ExpressiveStickerContentItemView xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:background="@drawable/compose2o_tile_background"
    android:layout_width="@dimen/sticker_content_item_size"
    android:layout_height="@dimen/sticker_content_item_size"
    android:layout_margin="@dimen/category_content_item_margin"
    android:foreground="?attr/selectableItemBackground"
    android:contentDescription="@string/c2o_category_recent_expressive_stickers_image_content_description">
    <ImageView
        android:id="@+id/sticker_image"
        android:padding="4dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scaleType="centerInside"/>
    <include layout="@layout/compose2o_selectable_content_item_view"/>
</com.google.android.apps.messaging.ui.mediapicker.c2o.expressivesticker.ExpressiveStickerContentItemView>
