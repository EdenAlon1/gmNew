<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:id="@+id/zero_state_search_multiple_result_container"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <include
        android:id="@+id/zero_state_search_results_filter"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/zero_state_search_multiple_result_filter_view_margin_bottom"
        layout="@layout/zero_state_search_results_filter_view_coolranch"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/zero_state_search_multiple_result_relative_layout_margin_top">
        <TextView
            android:id="@+id/zero_state_search_chat_header"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/zero_state_search_chat_header_margin_bottom"
            android:text="@string/zero_state_search_chats_header"
            style="@style/ZeroStateSearchChatHeaderStyle"/>
        <android.support.v7.widget.RecyclerView
            android:id="@+id/zero_state_search_chat_results"
            android:paddingTop="@dimen/conversation_list_first_item_extra_padding"
            android:clipToPadding="false"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/zero_state_search_results_margin_bottom"
            android:dividerHeight="0px"
            android:layout_below="@+id/zero_state_search_chat_header"
            android:importantForAccessibility="no"
            android:paddingStart="@dimen/zero_state_search_text_result_start_end_padding_coolranch"
            android:paddingEnd="@dimen/zero_state_search_text_result_start_end_padding_coolranch"/>
        <TextView
            android:id="@+id/zero_state_search_chat_see_all"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/zero_state_search_chat_header_margin_bottom"
            style="@style/ZeroStateSearchChatSeeAllButtonStyle"/>
        <TextView
            android:id="@+id/zero_state_search_starred_text_header"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/zero_state_search_chat_header_margin_bottom"
            android:text="@string/zero_state_search_starred_text_header"
            android:layout_below="@+id/zero_state_search_chat_results"
            style="@style/ZeroStateSearchChatHeaderStyle"/>
        <android.support.v7.widget.RecyclerView
            android:id="@+id/zero_state_search_starred_text_results"
            android:paddingTop="@dimen/conversation_list_first_item_extra_padding"
            android:visibility="gone"
            android:clipToPadding="false"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/zero_state_search_results_margin_bottom"
            android:dividerHeight="0px"
            android:layout_below="@+id/zero_state_search_starred_text_header"
            android:importantForAccessibility="no"
            android:paddingStart="@dimen/zero_state_search_text_result_start_end_padding_coolranch"
            android:paddingEnd="@dimen/zero_state_search_text_result_start_end_padding_coolranch"/>
        <TextView
            android:id="@+id/zero_state_search_starred_text_see_all"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/zero_state_search_chat_results"
            style="@style/ZeroStateSearchChatSeeAllButtonStyle"/>
        <TextView
            android:id="@+id/zero_state_search_photo_header"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/zero_state_search_photos_header"
            android:layout_below="@+id/zero_state_search_starred_text_results"
            style="@style/ZeroStateSearchChatHeaderStyle"/>
        <android.support.v7.widget.RecyclerView
            android:id="@+id/zero_state_search_photo_results"
            android:clipToPadding="false"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/zero_state_search_media_results_margin_bottom"
            android:dividerHeight="0px"
            android:layout_below="@+id/zero_state_search_photo_header"
            android:importantForAccessibility="no"
            android:paddingStart="@dimen/zero_state_search_text_result_start_end_padding_coolranch"
            android:paddingEnd="@dimen/zero_state_search_text_result_start_end_padding_coolranch"/>
        <TextView
            android:id="@+id/zero_state_search_photo_see_all"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/zero_state_search_starred_text_results"
            style="@style/ZeroStateSearchChatSeeAllButtonStyle"/>
        <TextView
            android:id="@+id/zero_state_search_video_header"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/zero_state_search_videos_header"
            android:layout_below="@+id/zero_state_search_photo_results"
            style="@style/ZeroStateSearchChatHeaderStyle"/>
        <android.support.v7.widget.RecyclerView
            android:id="@+id/zero_state_search_video_results"
            android:clipToPadding="false"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/zero_state_search_media_results_margin_bottom"
            android:dividerHeight="0px"
            android:layout_below="@+id/zero_state_search_video_header"
            android:importantForAccessibility="no"
            android:paddingStart="@dimen/zero_state_search_text_result_start_end_padding_coolranch"
            android:paddingEnd="@dimen/zero_state_search_text_result_start_end_padding_coolranch"/>
        <TextView
            android:id="@+id/zero_state_search_video_see_all"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/zero_state_search_photo_results"
            style="@style/ZeroStateSearchChatSeeAllButtonStyle"/>
        <TextView
            android:id="@+id/zero_state_search_locations_header"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/zero_state_search_locations_header"
            android:layout_below="@+id/zero_state_search_video_results"
            style="@style/ZeroStateSearchChatHeaderStyle"/>
        <android.support.v7.widget.RecyclerView
            android:id="@+id/zero_state_search_location_results"
            android:clipToPadding="false"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/zero_state_search_results_margin_bottom"
            android:dividerHeight="0px"
            android:layout_below="@+id/zero_state_search_locations_header"
            android:importantForAccessibility="no"
            android:paddingStart="@dimen/zero_state_search_text_result_start_end_padding_coolranch"
            android:paddingEnd="@dimen/zero_state_search_text_result_start_end_padding_coolranch"/>
        <TextView
            android:id="@+id/zero_state_search_locations_see_all"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/zero_state_search_video_results"
            style="@style/ZeroStateSearchChatSeeAllButtonStyle"/>
        <TextView
            android:id="@+id/zero_state_search_links_header"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/zero_state_search_links_header"
            android:layout_below="@+id/zero_state_search_location_results"
            style="@style/ZeroStateSearchChatHeaderStyle"/>
        <android.support.v7.widget.RecyclerView
            android:id="@+id/zero_state_search_link_results"
            android:clipToPadding="false"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/zero_state_search_results_margin_bottom"
            android:dividerHeight="0px"
            android:layout_below="@+id/zero_state_search_links_header"
            android:importantForAccessibility="no"
            android:paddingStart="@dimen/zero_state_search_text_result_start_end_padding_coolranch"
            android:paddingEnd="@dimen/zero_state_search_text_result_start_end_padding_coolranch"/>
        <TextView
            android:id="@+id/zero_state_search_links_see_all"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/zero_state_search_location_results"
            style="@style/ZeroStateSearchChatSeeAllButtonStyle"/>
        <TextView
            android:id="@+id/zero_state_search_contact_header"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/zero_state_search_chat_header_margin_bottom"
            android:text="@string/zero_state_search_contact_header"
            android:layout_below="@+id/zero_state_search_link_results"
            style="@style/ZeroStateSearchChatHeaderStyle"/>
        <android.support.v7.widget.RecyclerView
            android:id="@+id/zero_state_search_contact_results"
            android:clipToPadding="false"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:dividerHeight="0px"
            android:layout_below="@+id/zero_state_search_contact_header"
            android:importantForAccessibility="no"
            android:paddingStart="@dimen/zero_state_search_text_result_start_end_padding_coolranch"
            android:paddingEnd="@dimen/zero_state_search_text_result_start_end_padding_coolranch"/>
        <TextView
            android:id="@+id/zero_state_search_contact_see_all"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/zero_state_search_link_results"
            style="@style/ZeroStateSearchChatSeeAllButtonStyle"/>
    </RelativeLayout>
</LinearLayout>
