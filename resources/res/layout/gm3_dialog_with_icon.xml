<?xml version="1.0" encoding="utf-8"?>
<com.google.android.libraries.internal.growth.growthkit.internal.ui.impl.gm.dialogs.MaxDimensionsLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_gravity="bottom|center_horizontal"
    android:id="@+id/gm_dialog_container"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <RelativeLayout
        android:orientation="vertical"
        android:id="@+id/gm3_dialog_with_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">
        <include
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentTop="true"
            layout="@layout/gm_dialog_content"/>
        <ImageView
            android:id="@+id/gm_dialog_image"
            android:layout_width="@dimen/growthkit_gm3_icon_size"
            android:layout_height="@dimen/growthkit_gm3_icon_size"
            android:layout_marginTop="24dp"
            android:scaleType="fitCenter"
            android:layout_alignParentTop="true"
            android:layout_centerHorizontal="true"/>
    </RelativeLayout>
</com.google.android.libraries.internal.growth.growthkit.internal.ui.impl.gm.dialogs.MaxDimensionsLinearLayout>
