<?xml version="1.0" encoding="utf-8"?>
<com.google.android.apps.messaging.ui.generic.spannedautocomplete.SpannedAutoCompleteList xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:id="@+id/spanned_autocomplete_list"
    android:visibility="gone"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <View
        android:id="@+id/spanned_autocomplete_list_header"
        android:background="?attr/colorOutlineVariant"
        android:focusable="false"
        android:layout_width="match_parent"
        android:layout_height="1dp"/>
    <ListView
        android:orientation="vertical"
        android:id="@+id/spanned_autocomplete_listview"
        android:paddingTop="@dimen/spanned_autocomplete_list_padding_vertical"
        android:paddingBottom="@dimen/spanned_autocomplete_list_padding_vertical"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:divider="@null"
        android:layout_weight="1"/>
</com.google.android.apps.messaging.ui.generic.spannedautocomplete.SpannedAutoCompleteList>
