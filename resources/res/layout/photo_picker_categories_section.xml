<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <com.google.android.material.textview.MaterialTextView
        android:textAppearance="?attr/textAppearanceSubhead2"
        android:textColor="?attr/colorOnBackground"
        android:gravity="center_vertical"
        android:id="@+id/photo_picker_categories_section_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/photo_picker_art_home_section_title_bottom_margin"
        android:text="@string/op3_picker_all_collections"
        android:drawablePadding="@dimen/photo_picker_art_section_title_drawable_padding"
        android:drawableTint="?attr/colorOnSurfaceVariant"
        android:layout_marginHorizontal="@dimen/photo_picker_art_home_screen_margin"
        app:drawableStartCompat="@drawable/quantum_gm_ic_folder_vd_theme_24"/>
    <LinearLayout
        android:orientation="vertical"
        android:id="@+id/photo_picker_categories_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
</merge>
