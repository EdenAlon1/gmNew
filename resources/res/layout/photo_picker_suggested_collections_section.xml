<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <com.google.android.material.textview.MaterialTextView
        android:textAppearance="?attr/textAppearanceSubhead2"
        android:textColor="?attr/colorOnBackground"
        android:gravity="center_vertical"
        android:id="@+id/photo_picker_suggested_collections_section_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="12dp"
        android:text="@string/op3_more_to_explore"
        android:drawablePadding="@dimen/photo_picker_art_section_title_drawable_padding"
        android:drawableTint="?attr/colorOnSurfaceVariant"
        app:drawableStartCompat="@drawable/quantum_gm_ic_collections_vd_theme_24"/>
    <android.support.constraint.ConstraintLayout
        android:id="@+id/photo_picker_collections_mixed_grid"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <androidx.constraintlayout.widget.Guideline
            android:orientation="vertical"
            android:id="@+id/guideline_half_width"
            android:tag="guideline"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintGuide_percent="0.5"/>
        <include
            android:id="@+id/photo_picker_suggested_collection1"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toTopOf="@+id/photo_picker_suggested_collection4"
            app:layout_constraintDimensionRatio="h,5:6"
            app:layout_constraintEnd_toStartOf="@+id/guideline_half_width"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            layout="@layout/photo_picker_suggested_collection_image"/>
        <include
            android:id="@+id/photo_picker_suggested_collection2"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toTopOf="@+id/photo_picker_suggested_collection3"
            app:layout_constraintDimensionRatio="h,15:12"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/guideline_half_width"
            app:layout_constraintTop_toTopOf="parent"
            layout="@layout/photo_picker_suggested_collection_image"/>
        <include
            android:id="@+id/photo_picker_suggested_collection3"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toTopOf="@+id/photo_picker_suggested_collection4"
            app:layout_constraintDimensionRatio="h,15:6"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/guideline_half_width"
            app:layout_constraintTop_toBottomOf="@+id/photo_picker_suggested_collection2"
            layout="@layout/photo_picker_suggested_collection_image_two_lines"/>
        <include
            android:id="@+id/photo_picker_suggested_collection4"
            android:layout_width="match_parent"
            android:layout_height="100dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/photo_picker_suggested_collection1"
            app:layout_constraintWidth_default="percent"
            app:layout_constraintWidth_percent="1"
            layout="@layout/photo_picker_suggested_collection_image"/>
        <include
            android:id="@+id/photo_picker_suggested_collection5"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toTopOf="@+id/photo_picker_suggested_collection8"
            app:layout_constraintDimensionRatio="h,1:1"
            app:layout_constraintEnd_toStartOf="@+id/photo_picker_suggested_collection6"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/photo_picker_suggested_collection4"
            layout="@layout/photo_picker_suggested_collection_image"/>
        <include
            android:id="@+id/photo_picker_suggested_collection6"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toTopOf="@+id/photo_picker_suggested_collection8"
            app:layout_constraintDimensionRatio="h,1:1"
            app:layout_constraintEnd_toStartOf="@+id/photo_picker_suggested_collection7"
            app:layout_constraintStart_toEndOf="@+id/photo_picker_suggested_collection5"
            app:layout_constraintTop_toBottomOf="@+id/photo_picker_suggested_collection4"
            layout="@layout/photo_picker_suggested_collection_image"/>
        <include
            android:id="@+id/photo_picker_suggested_collection7"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toTopOf="@+id/photo_picker_suggested_collection9"
            app:layout_constraintDimensionRatio="h,1:1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/photo_picker_suggested_collection6"
            app:layout_constraintTop_toBottomOf="@+id/photo_picker_suggested_collection4"
            layout="@layout/photo_picker_suggested_collection_image"/>
        <include
            android:id="@+id/photo_picker_suggested_collection8"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintDimensionRatio="h,2:1"
            app:layout_constraintEnd_toStartOf="@+id/guideline_half_width"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/photo_picker_suggested_collection5"
            layout="@layout/photo_picker_suggested_collection_image"/>
        <include
            android:id="@+id/photo_picker_suggested_collection9"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintDimensionRatio="h,2:1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/guideline_half_width"
            app:layout_constraintTop_toBottomOf="@+id/photo_picker_suggested_collection7"
            layout="@layout/photo_picker_suggested_collection_image"/>
    </android.support.constraint.ConstraintLayout>
</merge>
