<?xml version="1.0" encoding="utf-8"?>
<com.google.android.apps.messaging.ui.mediapicker.c2o.gallery.FullscreenGalleryItemView xmlns:android="http://schemas.android.com/apk/res/android"
    android:gravity="center"
    android:orientation="vertical"
    android:background="@drawable/compose2o_tile_background"
    android:focusable="true"
    android:clickable="true"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="@dimen/category_content_item_margin"
    android:foreground="?attr/selectableItemBackground"
    android:contentDescription="@string/c2o_category_gallery_content_description">
    <ImageView
        android:id="@+id/full_screen_gallery_item_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/quantum_gm_ic_photo_library_vd_theme_24"
        android:tint="?attr/colorPrimaryBrandIcon"
        android:importantForAccessibility="no"/>
    <TextView
        android:textSize="@dimen/camera_gallery_text_size"
        android:textColor="?attr/colorOnSurface"
        android:gravity="center"
        android:id="@+id/full_screen_gallery_item_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/c2o_content_item_vertical_spacing_icon_text"
        android:text="@string/c2o_category_name_gallery"
        android:importantForAccessibility="no"/>
</com.google.android.apps.messaging.ui.mediapicker.c2o.gallery.FullscreenGalleryItemView>
