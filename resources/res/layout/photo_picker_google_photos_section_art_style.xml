<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <com.google.android.material.textview.MaterialTextView
        android:textAppearance="?attr/textAppearanceOverline"
        android:textColor="@color/photo_picker_header_title_text_color"
        android:id="@+id/photo_picker_suggested_section_title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/photo_picker_header_top_margin"
        android:layout_marginBottom="@dimen/photo_picker_header_bottom_margin"
        android:layout_marginHorizontal="10dp"/>
    <TableLayout
        android:id="@+id/photo_picker_suggested_photos_grid"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="2dp"
        android:stretchColumns="*"
        android:layout_marginHorizontal="6dp"/>
</merge>
