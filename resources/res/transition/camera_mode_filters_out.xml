<?xml version="1.0" encoding="utf-8"?>
<transitionSet xmlns:android="http://schemas.android.com/apk/res/android">
    <transitionSet
        android:interpolator="@interpolator/mtrl_fast_out_slow_in"
        android:duration="250">
        <transitionSet>
            <changeBounds/>
            <changeTransform/>
            <transition class="com.google.android.apps.messaging.ui.mediapicker.camera.ChangeTint"/>
            <targets>
                <target android:targetId="@+id/switch_camera"/>
                <target android:targetId="@+id/shutter_button_ring"/>
                <target android:targetId="@+id/shutter_button_fill"/>
            </targets>
        </transitionSet>
        <transitionSet>
            <changeBounds/>
            <transition class="com.google.android.apps.messaging.ui.mediapicker.camera.Scale"/>
            <targets>
                <target android:targetId="@+id/shutter_button_video_dot"/>
            </targets>
        </transitionSet>
    </transitionSet>
    <transitionSet android:duration="167">
        <fade/>
        <targets>
            <target android:targetId="@+id/shutter_button_fill"/>
        </targets>
    </transitionSet>
</transitionSet>
