<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:left="1dp"
        android:top="1dp"
        android:right="1dp"
        android:bottom="1dp">
        <shape android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="?attr/colorHairline"/>
            <gradient
                android:startColor="@color/google_transparent"
                android:endColor="@color/google_transparent"
                android:angle="0"
                android:centerColor="@color/google_transparent"/>
            <corners android:radius="@dimen/photo_picker_corner_crop_radius"/>
        </shape>
    </item>
</layer-list>
